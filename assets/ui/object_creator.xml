<window title="Objects" x="20" y="20" width="80" height="80" script="exp.ui.object_creator">
  <frame>
    <repeat while="get_next_object">
      <text fn="object_text" button="set_object"></text>
    </repeat>
  </frame>
  <frame if="did_select_object">
    <text>Draw Description</text>
    <text fn="get_wireframe_value"></text>
  </frame>
</window>